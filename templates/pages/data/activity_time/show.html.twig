{% extends 'base.html.twig' %}

{% block title %}ActivityTime{% endblock %}

{% block body %}
    <h1>ActivityTime</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ activity_time.id }}</td>
            </tr>
            <tr>
                <th>StartAt</th>
                <td>{{ activity_time.startAt ? activity_time.startAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>EndAt</th>
                <td>{{ activity_time.endAt ? activity_time.endAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Distance</th>
                <td>{{ activity_time.distance }}</td>
            </tr>
            <tr>
                <th>Time</th>
                <td>{{ activity_time.time }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_main_data_activity_time_index') }}">back to list</a>

    <a href="{{ path('app_main_data_activity_time_edit', {'id': activity_time.id}) }}">edit</a>

    {{ include('main/data/activity_time/_delete_form.html.twig') }}
{% endblock %}
